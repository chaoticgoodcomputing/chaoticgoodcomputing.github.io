{
  "$schema": "./node_modules/nx/schemas/project-schema.json",
  "name": "workspace",
  "root": ".",
  "projectType": "library",
  "targets": {
    "precommit": {
      "//": "Root-level precommit that delegates to vault, which cascades dependencies properly",
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          "nx run vault:precommit"
        ],
        "parallel": false
      }
    }
  }
}
